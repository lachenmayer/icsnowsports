mixin committeeMembers(members)
  ul#members
    - each member in members
      li.member
        p.name= member[0]
        p.position= member[1]

mixin eventsTable(events)
  table
    tr
      th What?
      th Where?
      th When?
    - each event in events
      tr
        td.what= event[0]
        td.where= event[1]
        td.when= event[2]

